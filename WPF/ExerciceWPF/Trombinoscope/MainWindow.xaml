<Window x:Class="Trombinoscope.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trombinoscope"
        xmlns:resx="clr-namespace:Trombinoscope.FichierResx"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="700">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top" Height="20">
                <MenuItem Name="menuTrombi" Header="{x:Static resx:Label.menuTrombi}" />
                <MenuItem Name="menuEmployés" Header="{x:Static resx:Label.menuEmployés}" />
            </Menu>
            <ContentControl Name="contentCtrl">
                
            </ContentControl>
        </DockPanel>
    </Grid>
</Window>
